<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gfhgfhfg"
    tools:context=".PolicyActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:gravity="center_horizontal"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="linkedin"
            android:src="@drawable/ic_linkedin_1_"
            tools:layout_editor_absoluteX="228dp"
            tools:layout_editor_absoluteY="654dp" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="github"
            android:src="@drawable/ic_github_2_"
            tools:layout_editor_absoluteX="114dp"
            tools:layout_editor_absoluteY="654dp" />
    </LinearLayout>

</LinearLayout>